{"ast":null,"code":"import io from 'socket.io-client';\nimport axios from 'axios';\nimport { mapState } from 'vuex';\nexport default {\n  name: \"EstoqueView\",\n  data() {\n    return {\n      produtos: []\n    };\n  },\n  methods: {\n    computed: {\n      ...mapState(['isActive'])\n    },\n    async getProdutos() {\n      try {\n        const response = await axios.get('http://localhost:5000/get_produtos');\n        console.log('Data from server:', response.data);\n        this.produtos = response.data;\n      } catch (error) {\n        console.error('Error fetching data:', error);\n      }\n    },\n    async updateDatabase() {\n      try {\n        // Example data for the POST request\n        const postData = {\n          cod_barra: '76548976445',\n          peso: '866'\n        };\n\n        // Make a POST request to update the database\n        await axios.post('http://localhost:5000/update_database', postData);\n\n        // Fetch updated data after the database is updated\n        //await this.getProdutos();\n      } catch (error) {\n        console.error('Error updating database:', error);\n      }\n    }\n  },\n  mounted() {\n    this.getProdutos();\n    const socket = io('http://localhost:5000');\n    socket.on('database_updated', data => {\n      console.log('database_updated:', data);\n      this.getProdutos();\n    });\n  }\n};","map":{"version":3,"names":["io","axios","mapState","name","data","produtos","methods","computed","getProdutos","response","get","console","log","error","updateDatabase","postData","cod_barra","peso","post","mounted","socket","on"],"sources":["src/components/EstoqueView.vue"],"sourcesContent":["<template>\r\n    <div class=\"fundo\" :class=\"{ 'isActive': this.$store.state.isActive }\">\r\n        <div class=\"teste\" >\r\n            <ul class=\"listaItens\">\r\n                <li class=\"items\" v-for=\"produto in produtos\" :key=\"produto.indice\">\r\n                    {{ produto.litragem }}\r\n                </li>\r\n            </ul>\r\n            <button @click=\"updateDatabase()\">Update Database</button>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport io from 'socket.io-client'\r\nimport axios from 'axios';\r\nimport { mapState } from 'vuex'\r\nexport default {\r\n    name: \"EstoqueView\",\r\n    data(){\r\n        return{\r\n            produtos: [],\r\n        }\r\n    },\r\n    methods: {\r\n        computed: {\r\n            ...mapState(['isActive'])\r\n        },\r\n        async getProdutos() {\r\n            try {\r\n                const response = await axios.get('http://localhost:5000/get_produtos');\r\n                console.log('Data from server:', response.data);        \r\n                this.produtos = response.data;\r\n            } catch (error) {\r\n                console.error('Error fetching data:', error);\r\n            }\r\n        },\r\n        async updateDatabase() {\r\n            try {\r\n                // Example data for the POST request\r\n                const postData = {\r\n                    cod_barra: '76548976445',\r\n                    peso: '866'\r\n                };\r\n\r\n                // Make a POST request to update the database\r\n                await axios.post('http://localhost:5000/update_database', postData);\r\n\r\n                // Fetch updated data after the database is updated\r\n                //await this.getProdutos();\r\n            } catch (error) {\r\n                console.error('Error updating database:', error);\r\n            }\r\n        },\r\n    },\r\n    mounted(){\r\n        this.getProdutos();\r\n        const socket = io('http://localhost:5000');\r\n        socket.on('database_updated',(data) =>{\r\n            console.log('database_updated:', data);\r\n            this.getProdutos();\r\n        })\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n.fundo{\r\n    background-color: #ffffff;\r\n    //width: calc(100% - 85px);\r\n    margin-left: auto;\r\n    border-radius: 20px;\r\n    margin-top: 10px;\r\n    margin-right: 0px;\r\n    height: auto;\r\n    transition: width 500ms ease;\r\n    &.isActive {\r\n        //width: calc(100% - 245px);\r\n        transition: width 500ms ease;\r\n    }\r\n}\r\n\r\n.items {\r\n    background-color: black;\r\n    //padding: 80px 100px;\r\n    width: 150px;\r\n    height: 300px;\r\n    margin: 20px 10px 0px;\r\n    border-radius: 20px;\r\n    list-style-type: none;\r\n    padding-top: 50px;\r\n}\r\n\r\n.listaItens {\r\n    display: flex;\r\n    flex-direction: row;\r\n    flex-wrap: wrap;\r\n    justify-content: space-evenly;\r\n    padding-left: 10px;\r\n    padding-right: 10px;\r\n}</style>"],"mappings":"AAcA,OAAAA,EAAA;AACA,OAAAC,KAAA;AACA,SAAAC,QAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,QAAA;IACA;EACA;EACAC,OAAA;IACAC,QAAA;MACA,GAAAL,QAAA;IACA;IACA,MAAAM,YAAA;MACA;QACA,MAAAC,QAAA,SAAAR,KAAA,CAAAS,GAAA;QACAC,OAAA,CAAAC,GAAA,sBAAAH,QAAA,CAAAL,IAAA;QACA,KAAAC,QAAA,GAAAI,QAAA,CAAAL,IAAA;MACA,SAAAS,KAAA;QACAF,OAAA,CAAAE,KAAA,yBAAAA,KAAA;MACA;IACA;IACA,MAAAC,eAAA;MACA;QACA;QACA,MAAAC,QAAA;UACAC,SAAA;UACAC,IAAA;QACA;;QAEA;QACA,MAAAhB,KAAA,CAAAiB,IAAA,0CAAAH,QAAA;;QAEA;QACA;MACA,SAAAF,KAAA;QACAF,OAAA,CAAAE,KAAA,6BAAAA,KAAA;MACA;IACA;EACA;EACAM,QAAA;IACA,KAAAX,WAAA;IACA,MAAAY,MAAA,GAAApB,EAAA;IACAoB,MAAA,CAAAC,EAAA,qBAAAjB,IAAA;MACAO,OAAA,CAAAC,GAAA,sBAAAR,IAAA;MACA,KAAAI,WAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}